    ns1:nextItem <http://pippo.it/birda-data/PersonNormal-Form-el2> .

<http://pippo.it/birda-data/PersonNormal-Form-el2> ns1:itemContent <http://pippo.it/birda-data/FamilyName-TextInput> ;
    ns1:nextItem <http://pippo.it/birda-data/PersonNormal-Form-el3> .

<http://pippo.it/birda-data/PersonNormal-Form-el3> ns1:itemContent <http://pippo.it/birda-data/Gender-TextInput> ;
    ns1:nextItem <http://pippo.it/birda-data/PersonNormal-Form-el4> .

<http://pippo.it/birda-data/PersonNormal-Form-el4> ns1:itemContent <http://pippo.it/birda-data/PersonKnowsSubForm-SubForm> .

<http://pippo.it/birda-data/PersonNormal-Form-local-name-list-el1> ns1:itemContent <http://pippo.it/birda-data/GivenName-TextInput> ;
    ns1:nextItem <http://pippo.it/birda-data/PersonNormal-Form-local-name-list-el2> .

<http://pippo.it/birda-data/PersonNormal-Form-local-name-list-el2> ns1:itemContent <http://pippo.it/birda-data/FamilyName-TextInput> .

<http://pippo.it/birda-data/FamilyName-TextInput> a ns2:TextInput ;
    ns2:atLeast 1 ;
    ns2:hasLabel "Family Name"@en,
        "Cognome"@it ;
    ns2:mapsProperty <http://xmlns.com/foaf/0.1/familyName> .

<http://pippo.it/birda-data/GivenName-TextInput> a ns2:TextInput ;
    ns2:atLeast 1 ;
    ns2:hasLabel "Name"@en,
        "Nome"@it ;
    ns2:mapsProperty <http://xmlns.com/foaf/0.1/givenName> .

